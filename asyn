    <h1>JS async/await</h1>
    <p id="demo"></p>
        <script>
            async function mul(a,b)
{
    let response= await a*b;
    display (response);
}
function display(some)
{
    document.getElementById("demo").innerHTML=some;
}
mul (28,20)
</script>
