<body>
    <h1> JSON IN JS</h1>
    <p id="demo"></p>
    <script>
        var a={name: "Vidushi", age: 15};
        var myJSON=JSON.stringify(a);
        var b=JSON.parse(myJSON);
        document.getElementById("demo").innerHTML=b.name;
    </script>
</body>